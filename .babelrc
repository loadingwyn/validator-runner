{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["Chrome >= 37", "ff >= 45", "Safari > 8", "IE >= 9"],
          "node": "current"
        },
        "modules": false
      }
    ]
  ],
  "plugins": ["transform-class-properties", "transform-object-rest-spread"],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
